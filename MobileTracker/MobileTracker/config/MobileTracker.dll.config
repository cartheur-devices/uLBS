<configuration>
  
  <!-- This section contains the log4net configuration settings -->
  <log4net>

    <!-- Define some output appenders -->
    <appender name="RollingLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <param name="File" value="fwm_log.txt" />
      <param name="AppendToFile" value="true" />

      <param name="MaxSizeRollBackups" value="100000" />
      <param name="MaximumFileSize" value="100000" />
      <param name="RollingStyle" value="Size" />
      <param name="StaticLogFileName" value="true" /> 
 
      <layout type="log4net.Layout.PatternLayout">
        <param name="Header" value="[iFind Mobile log]\r\n"/>
        <param name="Footer" value="[iFind Mobile log]\r\n"/>
        <param name="ConversionPattern" value="%d [%t] %-5p %c [%x] - %m%n" />
      </layout>
    </appender>

    <!-- Setup the root category, add the appenders and set the default level -->
    <root>
      <level value="ALL" />
      <appender-ref ref="RollingLogFileAppender" />
      <!-- <appender-ref ref="A" /> -->
    </root>

  </log4net>
  
  <appSettings>
    <add key="ApplicationID" value="43" />
    <add key="AutoStart" value="true" />
    <add key="AutoConnect" value="true" />
    <add key="AutoPanic" value="false" />
    <add key="DisconnectGPSAfterPlot" value="120" />
    <add key="DomesticInterval" value="900" />
    <add key="DisconnectAfterPlot" value="true" />
    <add key="FailCountSMS" value="3" />
    <add key="GPS" value="true" />
    <add key="GPSMarginInMeters" value="100" />
    <add key="InternationalInterval" value="3600" />
    <add key="MaxSMS" value="0" />
    <add key="MinBatLevel" value="10" />
    <add key="NotMovingDelay" value="10" />
    <add key="DumpQueueSize" value="100" />
    <add key="PanicLocationsToSent" value="15" />
    <add key="DumpQueueInterval" value="3600" /><!-- set this value to 3600 to avoid drainage /> -->
    <add key="Port" value="48889" />
    <add key="PositionExchange" value="true" />
    <!--<add key="Server" value="62.148.174.22" />--><!--pointing to the live server -->
    <!--<add key="Server" value="62.148.174.12" />--><!-- pointing to the staging server -->
    <add key="Server" value="92.65.62.41" /><!-- pointing to the test server-->
    <add key="SettingsVisible" value="true" />
    <add key="SettingsPassword" value="fw" />
    <add key="SpeedAlertInKM" value="0" />
    <add key="VMN" value="+15133681140" />
    <add key="VMN2" value="+447786200540" />
    <add key="WizardVisible" value="false" /><!--toggle showing of install wizard-->
    <add key="Version" value ="6.0.4a-smug"/>
  </appSettings>

</configuration>
